{
  "cli": {
    "version": ">= 16.18.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": { "buildType": "apk" },
      "environment": "development"
    },
    "preview": {
      "distribution": "internal",
      "environment": "preview"
    },
    "production": {
      "autoIncrement": true,
       "environment": "production"
    },
    "my-profile": {
      "environment": "production"
    }
  },
  "submit": {
    "development": {},
    "disabled": true  // 全局禁用提交功能（部分 EAS 版本支持）
  }
}
