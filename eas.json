{
  "cli": {
    "version": ">= 16.18.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk" // 开发环境生成 APK
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk" // 开发环境生成 APK
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk" // 开发环境生成 APK
      }
    },
    "my-profile": {
      "android": {
        "buildType": "apk" // 开发环境生成 APK
      }
    }
  }
}
